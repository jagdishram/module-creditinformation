<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="jag_creditinformation_customer_info" resource="default" engine="innodb" comment="jag_creditinformation_customer_info Table">
		<column xsi:type="int" name="customer_info_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="customer_info_id"/>
		</constraint>
		<column name="customer_id" nullable="true" xsi:type="int" comment="customer_id"/>		
		<column name="name" nullable="true" xsi:type="varchar" comment="name" length="255"/>
		<column name="email" nullable="true" xsi:type="varchar" comment="email" length="255"/>
		<column name="credit_type" nullable="true" xsi:type="varchar" comment="credit_type" length="255"/>		
	</table>
</schema>
